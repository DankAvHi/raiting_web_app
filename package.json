{
     "name": "raiting_web_app",
     "version": "1.0.0",
     "description": "telegram web app for raiting",
     "main": "index.js",
     "scripts": {
          "db-refresh": "dotenv -e production.env -- npx prisma db pull && npx prisma generate",
          "build": "cross-env NODE_ENV=production  npm run build-client && npm run build-server",
          "build-client": "cd client && npm run build",
          "build-server": "npx rimraf dist && webpack",
          "start": "cross-env NODE_ENV=production  npm run build && node dist/index.js",
          "dev-db-refresh": "dotenv -e development.env -- npx prisma db pull && npx prisma generate",
          "dev-start": "cross-env NODE_ENV=development  npx nodemon index.ts",
          "dev-start-secure": "cross-env NODE_ENV=development SECURE=true npx nodemon index.ts"
     },
     "author": "DankAvHi",
     "license": "ISC",
     "dependencies": {
          "@prisma/client": "^4.2.0",
          "cors": "^2.8.5",
          "cross-env": "^7.0.3",
          "dotenv": "^16.0.1",
          "dotenv-cli": "^6.0.0",
          "express": "^4.18.1",
          "rimraf": "^3.0.2"
     },
     "devDependencies": {
          "@types/cors": "^2.8.12",
          "@types/express": "^4.17.13",
          "@types/node": "^18.6.5",
          "copy-webpack-plugin": "^11.0.0",
          "nodemon": "^2.0.19",
          "prisma": "^4.2.0",
          "ts-loader": "^9.3.1",
          "ts-node": "^10.9.1",
          "typescript": "^4.7.4",
          "webpack": "^5.74.0",
          "webpack-cli": "^4.10.0",
          "webpack-node-externals": "^3.0.0",
          "webpack-shell-plugin": "^0.5.0"
     }
}
